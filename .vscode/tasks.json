{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build",
        "type": "shell",
        "command": "cmake",
        "args": ["--build", "${workspaceFolder}/build", "--config", "'Debug Build'"],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "presentation": {
          "reveal": "always",
          "panel": "new"
        },
        "problemMatcher": "$gcc"
      }
    ]
  }
  